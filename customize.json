{
	"Header": [
		{ "selector": "#header", "type": "background-color", "label": "Background color" },
		{ "selector": "#site-title a", "type": "text", "label": "Title" },
		{ "selector": "#site-title a:link, #site-title a:active, #site-title a:visited", "type": "color", "label": "Title color" },
		{ "selector": "#site-title a:hover", "type": "color", "label": "Title link hover color" },
		/* Applies to Tagline text and Tagline color */
		{ "selector": "#site-description", "type": "text", "label": "Tagline" }, 
		{ "selector": "#site-description", "type": "color", "label": "Tagline color" },
		{}
	],
	"Search Form": [
		{ "selector": "#s", "type": "text", "label": "Form" },
		{ "selector": "#s", "type": "color", "label": "Form color" },
		{ "selector": "#s", "type": "background-color", "label": "Form background color" },
		{ "selector": "#s:focus", "type": "background-color", "label": "Form focused background color", "important": true },
		{ "selector": "#s", "type": "color", "label": "Form border color", "template": "$selector { border-color: $value; }" },
		{ "selector": "#searchsubmit", "type": "text", "label": "Button" },
		{ "selector": "#searchsubmit", "type": "color", "label": "Button color" },
		{}
	],
	"Menu: Primary": [
		{ "selector": "#access", "type": "background-color", "label": "Background color" },
		/* Top menu items */
		{ "selector": "#access .menu ul", "type": "text", "label": "Top item" },
		{ "selector": "#access .menu ul > li > a", "type": "color", "label": "Top item color" },
		{ "selector": "#access .menu ul > li > a:hover", "type": "color", "label": "Top item hover color" },
		{ "selector": "#access .menu ul > li > a", "type": "background-color", "label": "Top item background color" },
		{ "selector": "#access .menu ul > li > a:hover", "type": "background-color", "label": "Top item hover background color" },
		/* Sub menu items */
		{ "selector": "#access ul ul a", "type": "text", "label": "Submenu item" },
		{ "selector": "#access .menu ul ul > li a", "type": "color", "label": "Submenu item color" },
		{ "selector": "#access .menu ul ul > li a:hover", "type": "color", "label": "Submenu item hover color" },
		{ "selector": "#access .menu ul ul > li a", "type": "background-color", "label": "Submenu item background color" },
		{ "selector": "#access .menu ul ul > li a:hover", "type": "background-color", "label": "Submenu item hover background color" },
		{}
	],
	/* Create multiple sidebar sections if appropriate for the theme */
	"Sidebar": [
		{ "selector": "#primary ul > li > h3", "type": "text", "label": "Widget title" },
		{ "selector": "#primary ul > li > h3", "type": "color", "label": "Widget title color" },
		{ "selector": "#primary ul ul > li", "type": "text", "label": "Widget content" },
		{ "selector": "#primary ul ul > li", "type": "color", "label": "Widget content color" },
		{ "selector": "#primary ul ul > li a", "type": "color", "label": "Widget link color" },
		{ "selector": "#primary ul ul > li > a:hover", "type": "color", "label": "Widget link hover color" },
		{}
	],
	"Entry Headings": [
		/* Title */
		{ "selector": ".entry-title, .entry-title a", "type": "text", "label": "Title" },
		{ "selector": ".entry-title, .entry-title a", "type": "color", "label": "Title color" },
		{ "selector": ".entry-title, .entry-title a:hover", "type": "color", "label": "Title link hover color" },
		/* Headers */
		{ "selector": "#content .entry-content h1", "type": "text", "label": "Heading 1" },
		{ "selector": "#content .entry-content h1", "type": "color", "label": "Heading 1 color" },
		{ "selector": "#content .entry-content h2", "type": "text", "label": "Heading 2" },
		{ "selector": "#content .entry-content h2", "type": "color", "label": "Heading 2 color" },
		{ "selector": "#content .entry-content h3", "type": "text", "label": "Heading 3" },
		{ "selector": "#content .entry-content h3", "type": "color", "label": "Heading 3 color" },
		{ "selector": "#content .entry-content h4", "type": "text", "label": "Heading 4" },
		{ "selector": "#content .entry-content h4", "type": "color", "label": "Heading 4 color" },
		{ "selector": "#content .entry-content h5", "type": "text", "label": "Heading 5" },
		{ "selector": "#content .entry-content h5", "type": "color", "label": "Heading 5 color" },
		{ "selector": "#content .entry-content h6", "type": "text", "label": "Heading 6" },
		{ "selector": "#content .entry-content h6", "type": "color", "label": "Heading 6 color" },
		{}
	],
	"Entry Content": [
		/* Paragraph */
		{ "selector": ".entry-content p, #respond p", "type": "text", "label": "Paragraph" },
		{ "selector": ".entry-content p", "type": "color", "label": "Paragraph color" },
		/* Links: Set for content and comments. Comment selection should override */
		{ "selector": ".entry-content a, #commentform a", "type": "color", "label": "Link color" },
		{ "selector": ".entry-content a:hover, #commentform a:hover", "type": "color", "label": "Link hover color" },
		/* Lists */
		{ "selector": ".entry-content li", "type": "text", "label": "List" },
		{ "selector": ".entry-content li", "type": "color", "label": "List color" },
		/* Preformatted */
		{ "selector": ".entry-content pre", "type": "text", "label": "Preformatted" },
		{ "selector": ".entry-content pre", "type": "color", "label": "Preformatted color" },
		{ "selector": ".entry-content pre", "type": "background-color", "label": "Preformatted Background color" },
		/* Blockquote */
		{ "selector": ".entry-content blockquote, .entry-content blockquote p", "type": "text", "label": "Blockquote" },
		{ "selector": ".entry-content blockquote p", "type": "color", "label": "Blockquote color" },
		/* Address */
		{ "selector": ".entry-content address", "type": "text", "label": "Address" },
		{ "selector": ".entry-content address", "type": "color", "label": "Address color" },
		/* Tables */
		{ "selector": ".entry-content table th", "type": "text", "label": "Table heading" },
		{ "selector": ".entry-content table th", "type": "color", "label": "Table heading color" },
		{ "selector": ".entry-content table th", "type": "background-color", "label": "Table heading background color" },
		{ "selector": ".entry-content table td", "type": "text", "label": "Table content" },
		{ "selector": ".entry-content table td", "type": "color", "label": "Table content color" },
		{ "selector": ".entry-content table td", "type": "background-color", "label": "Table content background color" },
		{}
	],
	"Entry Navigation": [
		{ "selector": "#content .navigation a", "type": "text", "label": "Previous/Next link" },
		{ "selector": "#content .navigation a", "type": "color", "label": "Previous/Next link color" },
		{ "selector": "#content .navigation a:hover", "type": "color", "label": "Previous/Next link hover color" },
		{ "selector": ".page-link a", "type": "text", "label": "Paging link" },
		{ "selector": ".page-link a", "type": "color", "label": "Paging link color" },
		{ "selector": ".page-link a:hover", "type": "color", "label": "Paging link hover color" },
		{ "selector": ".page-link a", "type": "background-color", "label": "Paging link background color" },
		{ "selector": ".page-link a:hover", "type": "background-color", "label": "Paging link hover background color" },
		{}
	],
	"Entry Meta": [
		{ "selector": ".entry-meta, .entry-utility", "type": "text", "label": "Meta" },
		{ "selector": ".entry-meta, .entry-utility", "type": "color", "label": "Meta color" },
		{ "selector": ".entry-meta a, .entry-utility a", "type": "color", "label": "Meta link color" },
		{ "selector": ".entry-meta a:hover, .entry-utility a:hover", "type": "color", "label": "Meta link hover color" },
		{ "selector": ".edit-link a", "type": "text", "label": "Edit link" },
		{ "selector": ".edit-link a", "type": "color", "label": "Edit link color" },
		{ "selector": ".edit-link a:hover", "type": "color", "label": "Edit link hover color" },
		{ "selector": ".edit-link a", "type": "background-color", "label": "Edit link background color" },
		{ "selector": ".edit-link a:hover", "type": "background-color", "label": "Edit link hover background color" },
		{}
	],
	"Comment": [
		/* Template used to remove speech bubble PNG and replace with CSS Triangle. Note use of sprintf arguement swapping: http://go.brain.st/16TeLbh */
		{ "selector": "#comments li.comment", "type": "background-color", "label": "Background color", "template": "%1$s { background-color: %2$s; } %1$s:before { content: ''; position:absolute; width: 0px; height: 0px; border-left: 28px solid transparent; border-right: 0px solid transparent; border-top: 27px solid %2$s; }" },
		{ "selector": "#comments .commentlist .comment-body p", "type": "text", "label": "Content" },
		{ "selector": "#comments .commentlist .comment-body p", "type": "color", "label": "Content color" },
		{ "selector": "#comments .commentlist .fn a", "type": "color", "label": "Link color" },
		{ "selector": "#comments .commentlist .fn a:hover", "type": "color", "label": "Link hover color" },
		{}
	],
	"Comment Meta": [
		{ "selector": "#comments-title", "type": "text", "label": "List Title" },
		{ "selector": "#comments .commentlist .comment-meta a", "type": "text", "label": "Meta" },
		{ "selector": "#comments .commentlist .comment-meta a", "type": "color", "label": "Meta color" },
		{ "selector": "#comments .commentlist .comment-meta a", "type": "color", "label": "Meta link color" },
		{ "selector": "#comments .comment-meta a:hover", "type": "color", "label": "Meta link hover color" },
		{ "selector": "#comments a.comment-reply-link", "type": "text", "label": "Reply link" },
		{ "selector": "#comments a.comment-reply-link", "type": "color", "label": "Reply link color" },
		{ "selector": "#comments a.comment-reply-link:hover", "type": "color", "label": "Reply link hover color" },
		{ "selector": "#comments a.comment-reply-link", "type": "background-color", "label": "Reply link background color" },
		{ "selector": "#comments a.comment-reply-link:hover", "type": "background-color", "label": "Reply link hover background color" },
		{}
	],
	"Comment Response": [
		{ "selector": "#respond", "type": "background-color", "label": "Background color" },
		{ "selector": "#respond #reply-title", "type": "text", "label": "Title" },
		{ "selector": "#respond #reply-title", "type": "color", "label": "Title color" },
		{ "selector": "#respond p", "type": "text", "label": "Description" },
		{ "selector": "#respond p", "type": "color", "label": "Description color" },
		{ "selector": "#commentform a", "type": "text", "label": "Description link" },
		{ "selector": "#commentform a", "type": "color", "label": "Description link color" },
		{ "selector": "#commentform a:hover", "type": "color", "label": "Description link hover color" },
		{ "selector": "#respond .comment-form-comment label", "type": "text", "label": "Label" },
		{ "selector": "#respond label", "type": "color", "label": "Label color" },
		{ "selector": "#respond label", "type": "background-color", "label": "Label background color" },
		{ "selector": "#respond textarea", "type": "text", "label": "Response" },
		{ "selector": "#respond textarea", "type": "color", "label": "Response color" },
		{ "selector": "#respond textarea", "type": "background-color", "label": "Response background color" },
		{ "selector": "#respond textarea:focus", "type": "background-color", "label": "Response focused background color" },
		{ "selector": "#respond textarea", "type": "color", "label": "Response border color", "template": "$selector { border-color: $value }" },
		{ "selector": "#respond input#submit", "type": "text", "label": "Submit button" },
		{ "selector": "#respond input#submit", "type": "color", "label": "Submit button color" },
		{ "selector": "#respond input#submit", "type": "background-color", "label": "Submit button background color" },
		{}
	],
	"Footer": [
		{ "selector": "#site-generator", "type": "background-color", "label": "Background color" },
		{ "selector": "#site-generator", "type": "color", "label": "Top border color", "template": "$selector { border-top-color: $value; }" },
		{ "selector": "#site-generator", "type": "text", "label": "Footer" },
		{ "selector": "#site-generator a", "type": "color", "label": "Footer link color" },
		{ "selector": "#site-generator a:hover", "type": "color", "label": "Footer link hover color" },
		{}
	]
}