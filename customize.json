		/* Comment link - Also set in Entry Content */
		/* Comment link - Effects comment body and meta links. Following sections should override */
		{ "selector": "#comments .even a", "type": "color", "label": "Link color" },
		{ "selector": "#comments .even a:hover", "type": "color", "label": "Link hover color" },
		{}
