		{ "selector": "#comments a", "type": "color", "label": "Link color" },
		{ "selector": "#comments a:hover", "type": "color", "label": "Link hover color" },
		/* Preformatted & Code - Also set in Entry Content  */
		/*{ "selector": ".comment-body pre, .comment-body code", "type": "text", "label": "Preformatted & code" },*/
		/*{ "selector": ".comment-body pre, .comment-body code", "type": "color", "label": "Preformatted & code color" },*/
		/*{ "selector": ".comment-body pre, .comment-body code", "type": "background-color", "label": "Preformatted & code background color" },*/
		{}
	],
