{
	"Header": [
		/* Background */
		{ "selector": "#header", "type": "background-color", "label": "Background color" },
		/* Title */
		{ "selector": "#site-title a", "type": "text", "label": "Title" },
		{ "selector": "#site-title a:link, #site-title a:active, #site-title a:visited", "type": "color", "label": "Title color" },
		{ "selector": "#site-title a:hover", "type": "color", "label": "Title link hover color" },
		/* Description */
		{ "selector": "#site-description", "type": "text", "label": "Description" }, 
		{ "selector": "#site-description", "type": "color", "label": "Description color" },
		{}
	],
	"Search Form": [
		/* Input */
		{ "selector": "#s", "type": "text", "label": "Form" },
		{ "selector": "#s", "type": "color", "label": "Form color" },
		{ "selector": "#s", "type": "background-color", "label": "Form background color" },
		/* Focus */
		{ "selector": "#s:focus", "type": "background-color", "label": "Form focused background color", "important": true },
		/* Border */
		{ "selector": "#s", "type": "border-color", "label": "Form border color" },
		/* Submit */
		{ "selector": "#searchsubmit", "type": "text", "label": "Button" },
		{ "selector": "#searchsubmit", "type": "color", "label": "Button color" },
		{ "selector": "#searchsubmit", "type": "background-color", "label": "Button background color" },
		{}
	],
	"Menu – Primary": [
		/* Background */
		{ "selector": "#access", "type": "background-color", "label": "Background color" },
		/* Top items */
		{ "selector": "#access .menu ul", "type": "text", "label": "Top item" },
		{ "selector": "#access .menu ul > li > a", "type": "color", "label": "Top item color" },
		{ "selector": "#access .menu ul > li > a:hover", "type": "color", "label": "Top item hover color" },
		{ "selector": "#access .menu ul > li > a", "type": "background-color", "label": "Top item background color" },
		{ "selector": "#access .menu ul > li > a:hover", "type": "background-color", "label": "Top item hover background color" },
		/* Sub items */
		{ "selector": "#access ul ul a", "type": "text", "label": "Submenu item" },
		{ "selector": "#access .menu ul ul > li a", "type": "color", "label": "Submenu item color" },
		{ "selector": "#access .menu ul ul > li a:hover", "type": "color", "label": "Submenu item hover color" },
		{ "selector": "#access .menu ul ul > li a", "type": "background-color", "label": "Submenu item background color" },
		{ "selector": "#access .menu ul ul > li a:hover", "type": "background-color", "label": "Submenu item hover background color" },
		{}
	],
	"Widgets – Sidebar": [
		/* Widget Title */
		{ "selector": "#primary .widget-title, #secondary .widget-title", "type": "text", "label": "Title" },
		{ "selector": "#primary .widget-title, #secondary .widget-title", "type": "color", "label": "Title color" },
		/* Widget Content */
		{ "selector": "#primary ul ul > li, #secondary ul ul > li", "type": "text", "label": "Content" },
		{ "selector": "#primary ul ul > li, #secondary ul ul > li", "type": "color", "label": "Content color" },
		/* Widget Link */
		{ "selector": "#primary ul ul > li a, #secondary ul ul > li a", "type": "color", "label": "Link color" },
		{ "selector": "#primary ul ul > li > a:hover, #secondary ul ul > li > a:hover", "type": "color", "label": "Link hover color" },
		{}
	],
	"Widgets – Footer": [
		/* Widget Title */
		{ "selector": "#footer-widget-area .widget-title", "type": "text", "label": "Title" },
		{ "selector": "#footer-widget-area .widget-title", "type": "color", "label": "Title color" },
		/* Widget Content */
		{ "selector": "#footer-widget-area .widget-area ul > li", "type": "text", "label": "Content" },
		{ "selector": "#footer-widget-area .widget-area ul > li", "type": "color", "label": "Content color" },
		/* Widget Link */
		{ "selector": "#footer-widget-area .widget-area ul > li a", "type": "color", "label": "Link color" },
		{ "selector": "#footer-widget-area .widget-area ul > li a:hover", "type": "color", "label": "Link hover color" },
		{}
	],
	"Entry – Headings": [
		/* Title */
		{ "selector": ".entry-title, .entry-title a", "type": "text", "label": "Title" },
		{ "selector": ".entry-title, .entry-title a", "type": "color", "label": "Title color" },
		{ "selector": ".entry-title, .entry-title a:hover", "type": "color", "label": "Title link hover color" },
		/* Headers */
		{ "selector": "#content .entry-content h1", "type": "text", "label": "Heading 1" },
		{ "selector": "#content .entry-content h1", "type": "color", "label": "Heading 1 color" },
		{ "selector": "#content .entry-content h2", "type": "text", "label": "Heading 2" },
		{ "selector": "#content .entry-content h2", "type": "color", "label": "Heading 2 color" },
		{ "selector": "#content .entry-content h3", "type": "text", "label": "Heading 3" },
		{ "selector": "#content .entry-content h3", "type": "color", "label": "Heading 3 color" },
		{ "selector": "#content .entry-content h4", "type": "text", "label": "Heading 4" },
		{ "selector": "#content .entry-content h4", "type": "color", "label": "Heading 4 color" },
		{ "selector": "#content .entry-content h5", "type": "text", "label": "Heading 5" },
		{ "selector": "#content .entry-content h5", "type": "color", "label": "Heading 5 color" },
		{ "selector": "#content .entry-content h6", "type": "text", "label": "Heading 6" },
		{ "selector": "#content .entry-content h6", "type": "color", "label": "Heading 6 color" },
		/* Archive Title */
		{ "selector": ".page-title, .page-title span", "type": "text", "label": "Archive title" },
		{ "selector": ".page-title, .page-title span", "type": "color", "label": "Archive title color" },
		{}
	],
	"Entry – Content": [
		/* Paragraph */
		{ "selector": ".entry-content p, #respond p", "type": "text", "label": "Paragraph" },
		{ "selector": ".entry-content p", "type": "color", "label": "Paragraph color" },
		/* Links - Set for content and comments. Comments section should override */
		{ "selector": ".entry-content a, #comments a", "type": "color", "label": "Link color" },
		{ "selector": ".entry-content a:hover, #comments a:hover", "type": "color", "label": "Link hover color" },
		/* Lists */
		{ "selector": ".entry-content li", "type": "text", "label": "List" },
		{ "selector": ".entry-content li", "type": "color", "label": "List color" },
		/* Small */
		{ "selector": ".entry-content small", "type": "text", "label": "Small" },
		{ "selector": ".entry-content small", "type": "color", "label": "Small color" },
		/* Preformatted & Code - Set for content and comments. Comments section might override */
		{ "selector": ".entry-content pre, .comment-body pre, .entry-content code, .comment-body code", "type": "text", "label": "Preformatted & code" },
		{ "selector": ".entry-content pre, .comment-body pre, .entry-content code, .comment-body code", "type": "color", "label": "Preformatted & code color" },
		{ "selector": ".entry-content pre, .comment-body pre, .entry-content code, .comment-body code", "type": "background-color", "label": "Preformatted & code background color" },
		/* Blockquote */
		{ "selector": ".entry-content blockquote, .entry-content blockquote p", "type": "text", "label": "Blockquote" },
		{ "selector": ".entry-content blockquote p", "type": "color", "label": "Blockquote color" },
		/* Address */
		{ "selector": ".entry-content address", "type": "text", "label": "Address" },
		{ "selector": ".entry-content address", "type": "color", "label": "Address color" },
		/* Table */
		{ "selector": ".entry-content th", "type": "text", "label": "Table heading" },
		{ "selector": ".entry-content th", "type": "color", "label": "Table heading color" },
		{ "selector": ".entry-content th", "type": "background-color", "label": "Table heading background color" },
		{ "selector": ".entry-content td, .entry-content td p", "type": "text", "label": "Table content" },
		{ "selector": ".entry-content td, .entry-content td p", "type": "color", "label": "Table content color" },
		{ "selector": ".entry-content td", "type": "background-color", "label": "Table content background color" },
		{}
	],
	"Entry – Navigation": [
		/* Previous/Next */
		{ "selector": "#content .navigation a", "type": "text", "label": "Previous/Next link" },
		{ "selector": "#content .navigation a", "type": "color", "label": "Previous/Next link color" },
		{ "selector": "#content .navigation a:hover", "type": "color", "label": "Previous/Next link hover color" },
		/* Paging */
		{ "selector": ".page-link, .page-link a", "type": "text", "label": "Paging" },
		{ "selector": ".page-link", "type": "color", "label": "Paging label color" },
		{ "selector": ".page-link a", "type": "color", "label": "Paging link color" },
		{ "selector": ".page-link a:hover", "type": "color", "label": "Paging link hover color" },
		{ "selector": ".page-link a", "type": "background-color", "label": "Paging link background color" },
		{ "selector": ".page-link a:hover", "type": "background-color", "label": "Paging link hover background color" },
		{}
	],
	"Entry – Meta": [
		/* Meta */
		{ "selector": ".entry-meta, .entry-utility", "type": "text", "label": "Meta" },
		{ "selector": ".entry-meta, .entry-utility", "type": "color", "label": "Meta color" },
		{ "selector": ".entry-meta a, .entry-utility a", "type": "color", "label": "Meta link color" },
		{ "selector": ".entry-meta a:hover, .entry-utility a:hover", "type": "color", "label": "Meta link hover color" },
		/* Edit link */
		{ "selector": ".edit-link a", "type": "text", "label": "Edit link" },
		{ "selector": ".edit-link a", "type": "color", "label": "Edit link color" },
		{ "selector": ".edit-link a:hover", "type": "color", "label": "Edit link hover color" },
		{ "selector": ".edit-link a", "type": "background-color", "label": "Edit link background color" },
		{ "selector": ".edit-link a:hover", "type": "background-color", "label": "Edit link hover background color" },
		{}
	],
	"Comments – Content": [
		{ "selector": "#comments .comment", "type": "background-color", "label": "Background color" },
		/* Comment content - This may need to change to effect comment meta as well */
		{ "selector": ".comment-body, .comment-body p", "type": "text", "label": "Content" },
		{ "selector": ".comment-body, .comment-body p, .comment-body h1, .comment-body h2, .comment-body h3, .comment-body h4, .comment-body h5, .comment-body h6, .comment-body blockquote cite, .comment-body th, .comment-body dt", "type": "color", "label": "Content color" },
		/* Comment link - Also set in Entry Content */
